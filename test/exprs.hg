assert = require("std").assert

a = 1
assert(a>=1)
assert(a>=0)
assert(a>0)
assert(not a>1)
assert(not a<1)
assert(a<2)
assert(a<=1)
assert(a<=2)
assert(a==1)
assert(not a==2)
assert(a!=1)

b = 2

assert(a==1 and b==2)
assert(a==1 or b==1)

assert(a*b==2)
assert(a-b==-2)
assert(a+b==a+b)
assert(a/b==0)
assert(b/a==2)
assert(b%a==0)
assert(a%b==1)

assert((a+b)*a/b*100+100/4+20*3%7==129)
